{
    "automock": true,
    "unmockedModulePathPatterns": [
        "<rootDir>/node_modules/*"
    ],
    "roots": [
        "<rootDir>/src"
    ],
    "globals": {
        "ts-jest": {
            "tsConfig": "tsconfig.json"
        }
    },
    "moduleDirectories": [
        "node_modules"
    ],
    "moduleFileExtensions": [
        "js",
        "jsx",
        "node",
        "ts",
        "tsx"
    ],
    "testMatch": [
        "**/__tests__/*.test.+(ts|tsx)"
    ],
    "collectCoverage": true,
    "transform": {
        "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "preset": "ts-jest"
}