    "eslint-plugin-react-hooks": "^1.6.0",
    "fibers": "^3.1.1",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin-alt": "^0.4.14",
    "fs-extra": "^7.0.1",
    "generate-changelog": "^1.7.1",
    "glob": "^7.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "4.0.0-alpha.2",
