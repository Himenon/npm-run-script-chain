{
  "name": "npm-run-script-chain",
  "version": "0.5.1",
  "description": "run-script chain",
  "keywords": [
    "chain",
    "package.json"
  ],
  "homepage": "https://github.com/Himenon/npm-run-script-chain#readme",
  "bugs": {
    "url": "https://github.com/Himenon/npm-run-script-chain/issues"
  },
  "repository": "Himenon/npm-run-script-chain",
  "license": "MIT",
  "author": {
    "name": "Himenon",
    "email": "k.himeno314@gmail.com",
    "url": "https://twitter.com/Himenon"
  },
  "files": [
    "lib",
    "build",
    "package.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "npm-run-script-chain": "./lib/cli.js",
    "nrsc": "./lib/cli.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "yarn run clean && yarn run build:scripts && node scripts/build.js && yarn run build:lib",
    "build:lib": "rimraf lib && run-p build:tsc build:style",
    "build:lib:watch": "run-p build:tsc:watch build:style:watch build:scripts:watch",
    "build:scripts": "tsc -p tsconfig.scripts.json",
    "build:scripts:watch": "yarn run build:scripts -w",
    "build:style": "webpack --config ./scripts/style.js",
    "build:style:watch": "webpack --config ./scripts/style.js --watch",
    "build:tsc": "tsc -p tsconfig.build.json",
    "build:tsc:watch": "yarn run build:tsc -w",
    "ci:notify:release": "ts-node ./scripts/notify.ts",
    "clean": "rimraf ./lib ./build",
    "cli": "node ./lib/cli.js",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "cruiser": "NODE_ENV=\"production\" npx dependency-cruiser --validate .dependency-cruiser.json --webpack-config ./scripts/webpack/webpack.config.js --ts-config=./tsconfig.json src",
    "develop": "yarn run build:scripts && yarn run server:dev",
    "format": "prettier --config .prettierrc --write src/*.{ts,tsx}",
    "lint": "eslint -c ./.eslintrc.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn run lint -- --fix",
    "server:dev": "node scripts/develop.js",
    "server:prod": "serve -s build",
    "start": "yarn run develop",
    "test": "yarn run cruiser && yarn run test:jest",
    "test:ci": "yarn run test && codecov",
    "test:jest": "jest -c ./jest.config.json",
    "test:watch": "yarn run test:jest -- --watch",
    "version_up:major": "changelog -M && git add CHANGELOG.md && git commit -m \"chore(change-log): updated CHANGELOG.md\" && npm version major",
    "version_up:minor": "changelog -m && git add CHANGELOG.md && git commit -m \"chore(change-log): updated CHANGELOG.md\" && npm version minor",
    "version_up:patch": "changelog -p && git add CHANGELOG.md && git commit -m \"chore(change-log): updated CHANGELOG.md\" && npm version patch"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "d3": "^5.9.2",
    "express": "^4.16.4",
    "meow": "^5.0.0",
    "opn": "^5.5.0",
    "paths-js": "^0.4.7",
    "react": "^16.8.5",
    "react-dom": "^16.8.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/chokidar": "^2.1.3",
    "@types/classnames": "^2.2.7",
    "@types/d3": "^5.7.1",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.0.5",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.11",
    "@types/jsdom": "^12.2.3",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^11.11.6",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/react": "^16.8.8",
    "@types/react-dev-utils": "^7.0.0",
    "@types/react-dom": "^16.8.3",
    "@types/terser-webpack-plugin": "^1.2.1",
    "@types/webpack": "^4.4.26",
    "@types/webpack-dev-server": "^3.1.4",
    "@types/webpack-manifest-plugin": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/eslint-plugin-tslint": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "autoprefixer": "^9.5.0",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "^6.1.1",
    "cache-loader": "^2.0.1",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "chokidar": "^2.1.5",
    "codecov": "^3.2.0",
    "css-loader": "^2.1.1",
    "dependency-cruiser": "^4.15.1",
    "eslint": "^5.15.3",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
