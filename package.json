{
  "name": "npm-run-script-chain",
  "version": "0.0.1",
  "description": "run-script chain",
  "keywords": [
    "chain",
    "package.json"
  ],
  "homepage": "https://github.com/git+https://github.com/Himenon/npm-run-script-chain.git#readme",
  "bugs": {
    "url": "https://github.com/git+https://github.com/Himenon/npm-run-script-chain.git/issues"
  },
  "repository": "git+https://github.com/Himenon/npm-run-script-chain.git",
  "license": "MIT",
  "author": {
    "name": "Himenon",
    "email": "k.himeno314@gmail.com",
    "url": "https://twitter.com/Himenon"
  },
  "files": [
    "lib",
    "package.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "npm-run-script-chain": "./lib/cli.js",
    "nrsc": "./lib/cli.js"
  },
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "yarn run clean && yarn run build:lib && yarn run build:scripts && node scripts/build.js",
    "build:lib": "tsc -p tsconfig.build.json",
    "build:lib:watch": "tsc -p tsconfig.build.json -w",
    "build:scripts": "tsc -p tsconfig.scripts.json",
    "build:scripts:watch": "tsc -p tsconfig.scripts.json -w",
    "build:style": "webpack --config ./scripts/style.js",
    "build:style:watch": "webpack --config ./scripts/style.js --watch",
    "ci:notify:release": "ts-node ./scripts/notify.ts",
    "clean": "rimraf ./lib ./build",
    "cli": "node ./lib/cli.js",
    "cli1": "chmod +x ./lib/cli.js && node ./lib/cli.js -f ./example/run-p-package.json -p 8005",
    "cli2": "chmod +x ./lib/cli.js && node ./lib/cli.js -f ./example/circle-package.json -p 8006",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "cruiser": "dependency-cruiser --validate .dependency-cruiser.json src",
    "develop": "run-p build:*:watch",
    "format": "prettier --config .prettierrc --write src/*.{ts,tsx}",
    "lint": "eslint -c ./.eslintrc.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn run lint -- --fix",
    "server:dev": "node --inspect scripts/develop.js",
    "server:prod": "serve -s build",
    "start": "yarn run develop",
    "test": "yarn run cruiser && yarn run test:jest",
    "test:ci": "yarn run test && codecov",
    "test:jest": "jest -c ./jest.config.json",
    "test:watch": "yarn run test:jest -- --watch",
    "version_up:major": "changelog -M && git add CHANGELOG.md && git commit -m \"chore(change-log): updated CHANGELOG.md\" && npm version major",
    "version_up:minor": "changelog -m && git add CHANGELOG.md && git commit -m \"chore(change-log): updated CHANGELOG.md\" && npm version minor",
    "version_up:patch": "changelog -p && git add CHANGELOG.md && git commit -m \"chore(change-log): updated CHANGELOG.md\" && npm version patch"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@types/classnames": "^2.2.7",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "d3": "^5.9.1",
    "express": "^4.16.4",
    "meow": "^5.0.0",
    "opn": "^5.4.0",
    "paths-js": "^0.4.7",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/d3": "^5.7.1",
    "@types/express": "^4.16.1",
    "@types/fs-extra": "^5.0.5",
    "@types/glob": "^7.1.1",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.11",
    "@types/jsdom": "^12.2.3",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^11.11.0",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/react": "^16.8.7",
    "@types/react-dev-utils": "^7.0.0",
    "@types/react-dom": "^16.8.2",
    "@types/terser-webpack-plugin": "^1.2.1",
    "@types/webpack": "^4.4.25",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-dev-server": "^3.1.4",
    "@types/webpack-manifest-plugin": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/eslint-plugin-tslint": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "autoprefixer": "^9.4.10",
    "bfj": "^6.1.1",
    "cache-loader": "^2.0.1",
    "codecov": "^3.2.0",
    "css-loader": "^2.1.1",
    "dependency-cruiser": "^4.13.3",
    "eslint": "^5.15.1",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin-alt": "^0.4.14",
    "fs-extra": "^7.0.1",
    "generate-changelog": "^1.7.1",
